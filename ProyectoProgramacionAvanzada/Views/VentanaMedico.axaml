<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        x:Class="ProyectoProgramacionAvanzada.Views.VentanaMedico"
        Title="Gestión de Médicos"
        Width="480" Height="450">
<StackPanel Margin="20">
        <TextBox x:Name="NombreTextBox" Watermark="Nombre" Margin="0, 0, 0, 5"/>
        <TextBox x:Name="CorreoTextBox" Watermark="Correo" Margin="0, 0, 0, 5"/>
        <TextBox x:Name="ConsultorioTextBox" Watermark="Consultorio" Margin="0, 0, 0, 5"/>
        <Button Content="Guardar" Click="GuardarButton_Click" Margin="0, 10, 0, 0"/>
        <Button Content="Cerrar" Click="CerrarButton_Click" Margin="0, 10, 0, 0"/>
    </StackPanel>
</Window>